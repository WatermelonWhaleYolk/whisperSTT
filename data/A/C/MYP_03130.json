        "term": [
            {
                "speaker_id": "1",
                "transcription": "뭐지?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 0.549
            },
            {
                "speaker_id": "1",
                "transcription": "아침 댓바람부터 얼탱이가 가출해서 돌아오질 않는군.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.559,
                "end": 4.739
            },
            {
                "speaker_id": "1",
                "transcription": "조막만한 에피타이저가 빨리 먹어 달라고 시위하는 건가?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.749,
                "end": 9.039
            },
            {
                "speaker_id": "2",
                "transcription": "뭐 임마 에피타이저?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 9.049,
                "end": 11.523
            },
            {
                "speaker_id": "2",
                "transcription": "너 언제부터 내 앞에서 그렇게 버르장머리 없이 지냈냐?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 11.533,
                "end": 14.703
            },
            {
                "speaker_id": "1",
                "transcription": "네가 뭔데 임마 너 뭐 돼?",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 14.713,
                "end": 18.641
            },
            {
                "speaker_id": "2",
                "transcription": "아 맞다.",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 18.651,
                "end": 19.768
            },
            {
                "speaker_id": "2",
                "transcription": "나 지금 용사였지?",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 19.778,
                "end": 21.058
            },
            {
                "speaker_id": "2",
                "transcription": "어 그게 내 내 내가 다 설명할 수 있다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 21.068,
                "end": 25.211
            },
            {
                "speaker_id": "1",
                "transcription": "어 그래?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 25.221,
                "end": 27.265
            },
            {
                "speaker_id": "2",
                "transcription": "내가 지금은 용사로 보일지언정 사실 난 마왕이야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 27.275,
                "end": 30.868
            },
            {
                "speaker_id": "2",
                "transcription": "워 원래 내 몸으로 환생했어야 했는데 문제가 생겼다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 30.878,
                "end": 34.675
            },
            {
                "speaker_id": "2",
                "transcription": "분명 신이 뭔가 장난을 친 거겠지.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 34.685,
                "end": 36.744
            },
            {
                "speaker_id": "2",
                "transcription": "너라도 날 믿어줘야 돼.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 36.754,
                "end": 38.375
            },
            {
                "speaker_id": "2",
                "transcription": "지금쯤이면 용사도 내 몸으로 환생했을텐데 마왕군이 위험할 거야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 38.385,
                "end": 42.306
            },
            {
                "speaker_id": "1",
                "transcription": "흠 그렇군.",
