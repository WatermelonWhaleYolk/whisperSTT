        "term": [
            {
                "speaker_id": "1",
                "transcription": "입단 테스트라니 뭐 여기서 드리블이라도 보여드려야 하나요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 3.210
            },
            {
                "speaker_id": "1",
                "transcription": "어떻게 하면 되는 거죠?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 3.220,
                "end": 4.265
            },
            {
                "speaker_id": "2",
                "transcription": "홀란드 너도 근본론이라고 한 번 쯤은 들어봤겠지?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.275,
                "end": 7.548
            },
            {
                "speaker_id": "1",
                "transcription": "근본론이요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.558,
                "end": 8.493
            },
            {
                "speaker_id": "1",
                "transcription": "그게 뭔가요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 8.503,
                "end": 9.310
            },
            {
                "speaker_id": "3",
                "transcription": "하하 짜식 이 선배가 대신 설명해주마.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 9.320,
                "end": 11.580
            },
            {
                "speaker_id": "3",
                "transcription": "근본론이란 유망주에게 근본이 있냐 없냐를 판별하는 논리로 총 4가지로 구성돼 있다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 11.590,
                "end": 16.212
            },
            {
                "speaker_id": "1",
                "transcription": "켈파 선배는 근본도 없으시면서 되게 잘 아시네요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 16.222,
                "end": 19.063
            },
            {
                "speaker_id": "3",
                "transcription": "조용히 안 해?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 19.073,
                "end": 19.842
            },
            {
                "speaker_id": "3",
                "transcription": "아무튼 첫 번째로 겉멋에 환장하였는가?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 19.852,
                "end": 22.081
            },
            {
                "speaker_id": "3",
                "transcription": "두 번째로 연상의 글래머러스한 여자친구가 있는가?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 22.091,
                "end": 24.804
            },
            {
                "speaker_id": "3",
                "transcription": "세 번째로 돈에 환장하였는가?",
                "sound_type": [
                    2,
                    3
                ],
