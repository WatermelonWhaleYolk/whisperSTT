        ],
        "term": [
            {
                "speaker_id": "1",
                "transcription": "아 아 들리세요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 1.117
            },
            {
                "speaker_id": "1",
                "transcription": "여기는 김덕배 아구에로 형님 들리세요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.127,
                "end": 3.599
            },
            {
                "speaker_id": "2",
                "transcription": "아 아 여기는 아구에로 덕배야 뭐 좀 찾았냐?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 3.609,
                "end": 6.785
            },
            {
                "speaker_id": "1",
                "transcription": "아뇨 여긴 아무 것도 없는 것 같습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 6.795,
                "end": 8.713
            },
            {
                "speaker_id": "1",
                "transcription": "그냥 빈 방이에요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 8.723,
                "end": 9.965
            },
            {
                "speaker_id": "2",
                "transcription": "여기도 마찬가지다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 9.975,
                "end": 11.161
            },
            {
                "speaker_id": "2",
                "transcription": "이것들이 먼저 눈치를 채고 빼돌린 건지 아무 것도 없다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 11.171,
                "end": 14.059
            },
            {
                "speaker_id": "1",
                "transcription": "교활한 구너놈들 아무튼 이제 어떡하죠 형님?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 14.069,
                "end": 17.051
            },
            {
                "speaker_id": "3",
                "transcription": "형님 형님",
                "sound_type": [
                    2,
                    3
                ],
                "start": 17.061,
                "end": 18.184
            },
            {
                "speaker_id": "1",
                "transcription": "쉿 들키면 어쩌려고 조용히 하란 말이야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 18.194,
                "end": 20.655
            },
            {
                "speaker_id": "3",
                "transcription": "형님 찾았습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 20.665,
                "end": 21.796
            },
            {
                "speaker_id": "3",
                "transcription": "구너파의 비밀 문서는 물론 아스날 요원 보고서까지 다 찾았어요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 21.806,
                "end": 25.417
            },
            {
                "speaker_id": "1",
                "transcription": "아니 그게 정말이야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 25.427,
                "end": 26.565
            },
            {
                "speaker_id": "1",
                "transcription": "어디서 찾았어?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 26.575,
                "end": 27.349
            },
            {
                "speaker_id": "3",
                "transcription": "회장실이 아니라 화장실이었습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 27.359,
                "end": 29.217
            },
            {
                "speaker_id": "3",
                "transcription": "화장실 청소 도구함에 모든 게 다 있었습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 29.227,
                "end": 31.654
            },
            {
                "speaker_id": "1",
                "transcription": "기밀 문서를 화장실에 숨겨놨다고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 31.664,
                "end": 33.706
            },
            {
                "speaker_id": "3",
                "transcription": "아이 그러게 말입니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 33.716,
                "end": 34.534
            },
            {
                "speaker_id": "1",
                "transcription": "하여간 구너파놈들",
                "sound_type": [
                    2,
                    3
                ],
                "start": 34.544,
                "end": 35.804
            },
            {
                "speaker_id": "1",
                "transcription": "아이 뭐 어찌됐든 얼른 챙겨서 나가자 지체할 시간이 없어.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 35.814,
                "end": 38.476
            },
            {
                "speaker_id": "1",
                "transcription": "아 아무튼 그동안 내가 미안했네 무작정 자네를 오해만 하고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 38.486,
                "end": 42.400
            }
        ]
    },
    "summary": "남성이 문서를 찾았다고 말하자 어디에서 찾았냐고 묻고 화장실에서 찾았다고 답한다.",
