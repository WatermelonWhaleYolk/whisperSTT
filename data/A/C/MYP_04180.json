        "term": [
            {
                "speaker_id": "1",
                "transcription": "그렇게 폭발한 그의 분노는 한동안 사그러들 줄 몰랐지만",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 4.688
            },
            {
                "speaker_id": "2",
                "transcription": "니 눈엔 내가 안 보이냐?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.698,
                "end": 6.761
            },
            {
                "speaker_id": "3",
                "transcription": "문 옆에 계셔서 못 봤습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 6.771,
                "end": 8.593
            },
            {
                "speaker_id": "2",
                "transcription": "나는 앞에 계셔도 못 봐.",
