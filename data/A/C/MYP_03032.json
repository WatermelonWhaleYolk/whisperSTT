        "term": [
            {
                "speaker_id": "1",
                "transcription": "일단 지켜봐요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 0.958
            },
            {
                "speaker_id": "1",
                "transcription": "얼마나 막 나가는지 알아야 솔루션을 하죠.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.968,
                "end": 3.263
            },
            {
                "speaker_id": "2",
                "transcription": "여기 사장님 여기 피자가 덜 익었는데 좀 더 익혀주실 수.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 3.273,
                "end": 6.294
            },
            {
                "speaker_id": "3",
                "transcription": "아이씨 먹어도 안 뒤지니까 그냥 먹어요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 6.304,
                "end": 8.839
            },
            {
                "speaker_id": "4",
                "transcription": "저기 사장님 피자가 나오긴 하는 건가요 지금 89분째 기다리고 있는데.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 8.849,
                "end": 12.498
            },
            {
                "speaker_id": "3",
                "transcription": "아오 피곤해 뒤지겠네.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 12.508,
                "end": 13.845
            },
            {
                "speaker_id": "3",
                "transcription": "야 부폰아 나와서 주문 좀 받아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 13.855,
                "end": 15.734
            },
            {
                "speaker_id": "5",
                "transcription": "아 요리사님이 나오면 피자는 누가 굽나요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 15.744,
                "end": 18.200
            },
            {
                "speaker_id": "1",
                "transcription": "90분이 넘어서도 피자가 안 나오면 뭐 환불 처리 해야죠.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 18.210,
                "end": 21.849
            },
            {
                "speaker_id": "6",
                "transcription": "저 새끼가 잘도 환불을 해주겠다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 21.859,
                "end": 23.666
            }
        ]
    },
