        "term": [
            {
                "speaker_id": "1",
                "transcription": "어이 맹그바 쟤들 뭐하는 거야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 1.860
            },
            {
                "speaker_id": "2",
                "transcription": "몰라 영양가라고는 하나도 없는 드리블만 오지게 치는 거 같은데",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.870,
                "end": 5.670
            },
            {
                "speaker_id": "3",
                "transcription": "오호호호 그 정도로는 우리 맹구특전대를 쓰러트릴 수 없을 겁니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.680,
                "end": 12.228
            },
            {
                "speaker_id": "4",
                "transcription": "이대로라면 유로파 각이다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 12.238,
                "end": 14.078
            },
            {
                "speaker_id": "4",
                "transcription": "별수 없군 이봐 손카로트",
                "sound_type": [
                    2,
                    3
                ],
                "start": 14.088,
                "end": 16.242
            },
            {
                "speaker_id": "5",
                "transcription": "예 스승님.",
