        ],
        "term": [
            {
                "speaker_id": "1",
                "transcription": "봐라.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 0.616
            },
            {
                "speaker_id": "2",
                "transcription": "이게 뭐죠? 팩토리 월드컵?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.626,
                "end": 2.138
            },
            {
                "speaker_id": "1",
                "transcription": "우리 같은 노동자들이 참여하는 월드컵이다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 2.148,
                "end": 4.970
            },
            {
                "speaker_id": "1",
                "transcription": "우리 중에 축구선수 되고 싶은 애들 많다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.980,
                "end": 7.589
            },
            {
                "speaker_id": "1",
                "transcription": "@이름1도 언젠간 맨유에서 뛸 거다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.599,
                "end": 10.058
            },
            {
                "speaker_id": "2",
                "transcription": "에이 이런 데서 뛴다고 누가 알아줘요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 10.068,
                "end": 12.045
            },
            {
                "speaker_id": "2",
                "transcription": "무슨 프리미어리그예요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 12.055,
                "end": 13.272
            },
            {
                "speaker_id": "1",
                "transcription": "싫으면 관둬라.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 13.282,
                "end": 14.532
            },
            {
                "speaker_id": "1",
                "transcription": "여기 머지사이드주에 리버풀과 에버튼에서 스카우트도 온댔는데",
                "sound_type": [
                    2,
                    3
                ],
                "start": 14.542,
                "end": 18.967
            },
            {
                "speaker_id": "2",
                "transcription": "뭐? 스카우트까지 온다고?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 18.977,
                "end": 20.425
            },
            {
                "speaker_id": "2",
                "transcription": "이 새끼 말을 믿을 수가 있어야지.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 20.435,
                "end": 22.341
            },
            {
                "speaker_id": "2",
                "transcription": "그래도 혹시 모르잖아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 22.351,
                "end": 23.528
            },
            {
                "speaker_id": "2",
                "transcription": "매일 밤마다 벽에 대고 공 차는 것도 지겨운데 밑져야 본전이겠지?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 23.538,
                "end": 26.950
            },
            {
                "speaker_id": "2",
                "transcription": "그래서 어떻게 참가하면 되는데요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 26.960,
                "end": 28.842
            },
            {
                "speaker_id": "1",
                "transcription": "우리 공장에 이미 김치 유나이티드라고 축구 팀 있다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 28.852,
                "end": 32.666
            },
            {
                "speaker_id": "1",
                "transcription": "@이름1가 주장이다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 32.676,
                "end": 34.219
            },
            {
                "speaker_id": "1",
                "transcription": "감독님 만나러 갈 거냐?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 34.229,
                "end": 36.055
            },
            {
                "speaker_id": "2",
                "transcription": "아니 시발 김치 유나이티드가 뭐예요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 36.065,
                "end": 38.315
            },
            {
                "speaker_id": "1",
                "transcription": "지금 김치 무시하냐?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 38.325,
                "end": 39.793
            },
            {
                "speaker_id": "1",
                "transcription": "우리 선수들 다들 잘한다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 39.803,
                "end": 41.754
            },
            {
                "speaker_id": "1",
                "transcription": "매일 밤 훈련한다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 41.764,
                "end": 43.116
            },
            {
                "speaker_id": "2",
                "transcription": "에이 시발 밑져야 본전이겠지.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 43.126,
                "end": 45.123
            },
            {
                "speaker_id": "2",
                "transcription": "아 예 뭐 그럼 감독님께 한번 가봅시다.",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 45.133,
                "end": 49.200
            }
        ]
    },
    "summary": "@이름1이 노동자들이 참여하는 월드컵인 팩토리 월드컵을 소개한 후 @이름1이 일하는 공장에 김치 유나이티드가 있다고 말한다.",
