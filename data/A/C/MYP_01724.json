        "term": [
            {
                "speaker_id": "1",
                "transcription": "이 세 가지를 섞었다가 싫어요 테러를 받을지도 모른다고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 3.134
