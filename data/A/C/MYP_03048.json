        "term": [
            {
                "speaker_id": "1",
                "transcription": "이렇게 돌아가시면 어떡해요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 1.541
            },
            {
                "speaker_id": "1",
                "transcription": "잘해드리지도 못했는데.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.551,
                "end": 3.804
            },
            {
                "speaker_id": "2",
                "transcription": "좋은 곳으로 가셨을 거예요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 3.814,
                "end": 5.508
            },
            {
                "speaker_id": "2",
                "transcription": "너무 슬퍼하지 마세요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.518,
                "end": 7.002
            },
            {
                "speaker_id": "1",
                "transcription": "좆두 형님.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.012,
                "end": 8.215
            },
            {
                "speaker_id": "2",
                "transcription": "나 날강두 사장님.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 8.225,
                "end": 11.346
            },
            {
                "speaker_id": "3",
                "transcription": "뭬 뭬야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 11.356,
                "end": 12.319
            },
            {
                "speaker_id": "3",
                "transcription": "내가 죽은 게야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 12.329,
                "end": 13.957
            },
            {
                "speaker_id": "4",
                "transcription": "그럼 누구겠어.",
