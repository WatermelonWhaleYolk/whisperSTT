        "term": [
            {
                "speaker_id": "1",
                "transcription": "자 여기가 바로 우리 김치 유나이티드의 훈련장이다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 3.182
            },
            {
                "speaker_id": "1",
                "transcription": "준비는 충분히 하고 왔겠지?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 3.192,
                "end": 5.191
            },
            {
                "speaker_id": "2",
                "transcription": "아 예 뭐 준비할 것도 없습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.201,
                "end": 7.190
            },
            {
                "speaker_id": "2",
                "transcription": "뭐야?",
