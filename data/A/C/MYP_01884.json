        ],
        "term": [
            {
                "speaker_id": "1",
                "transcription": "자 다시 호날두 선수의 차례입니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 2.050
            },
            {
                "speaker_id": "1",
                "transcription": "정답을 말씀해 주시죠.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 2.060,
                "end": 3.457
            },
            {
                "speaker_id": "2",
                "transcription": "린가드는 확실하게 14번이었어.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 3.467,
                "end": 5.388
            },
            {
                "speaker_id": "2",
                "transcription": "근데 카바니는 몇 번이지?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.398,
                "end": 7.024
            },
            {
                "speaker_id": "2",
                "transcription": "한 20번대라 해도 14를 뺀다면 대충 뭐 1자리 숫자일 테니까 마 마딸인가?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.034,
                "end": 11.863
            },
            {
                "speaker_id": "2",
                "transcription": "정답 앙토니 마딸",
                "sound_type": [
                    2,
                    3
                ],
                "start": 11.873,
                "end": 13.202
            },
            {
                "speaker_id": "1",
                "transcription": "틀렸습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 13.212,
                "end": 13.983
            },
            {
                "speaker_id": "1",
                "transcription": "참고로 맨유에 마딸이란 선수는 없습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 13.993,
                "end": 16.522
            },
            {
                "speaker_id": "1",
                "transcription": "마샬이면 모를까",
                "sound_type": [
                    2,
                    3
                ],
                "start": 16.532,
                "end": 17.761
            },
            {
                "speaker_id": "3",
                "transcription": "자 어떻게 하실래요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 17.771,
                "end": 19.109
            },
            {
                "speaker_id": "3",
                "transcription": "1대 더 콜",
                "sound_type": [
                    2,
                    3
                ],
                "start": 19.119,
                "end": 20.171
            },
            {
                "speaker_id": "2",
                "transcription": "얼른 안 때리고 뭐 하고 있어?",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 20.181,
                "end": 21.972
            },
            {
                "speaker_id": "1",
                "transcription": "다시 호날두 선수의 차례입니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 21.982,
                "end": 24.079
            },
            {
                "speaker_id": "1",
                "transcription": "특별히 힌트를 드리자면 아까 틀리셨던 마샬도 아닐뿐더러 자타공인 팀의 에이스로 인정받는 선수입니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 24.089,
                "end": 31.123
            },
            {
                "speaker_id": "2",
                "transcription": "에이스라고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 31.133,
                "end": 32.019
            },
            {
                "speaker_id": "2",
                "transcription": "그럼 주장 녀석인가?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 32.029,
                "end": 33.464
            },
            {
                "speaker_id": "2",
                "transcription": "정답 맥도날드",
                "sound_type": [
                    2,
                    3
                ],
                "start": 33.474,
                "end": 34.481
            },
            {
                "speaker_id": "1",
                "transcription": "틀렸습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 34.491,
                "end": 35.338
            },
            {
                "speaker_id": "1",
                "transcription": "참고로 맨유의 주장은 맥도날드가 아니라 매과이어입니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 35.348,
                "end": 38.674
            },
            {
                "speaker_id": "2",
                "transcription": "이런 시발",
                "sound_type": [
                    2,
                    3
                ],
                "start": 38.684,
                "end": 39.583
            },
            {
                "speaker_id": "2",
                "transcription": "어이 루카쿠 1대만 더 맞겠다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 39.593,
                "end": 41.151
            },
            {
                "speaker_id": "3",
                "transcription": "이랏샤이마세 호날두상",
                "sound_type": [
                    2,
                    3
                ],
                "start": 41.161,
                "end": 42.893
            },
            {
                "speaker_id": "3",
                "transcription": "뭐 어떻게 제가 카바니 선수 등번호라도 알려드려요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 42.903,
                "end": 46.170
            },
            {
                "speaker_id": "2",
                "transcription": "아이 그래 주면 나야 고맙지.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 46.180,
                "end": 47.699
            },
            {
                "speaker_id": "3",
                "transcription": "대신 이번엔 싸대기가 아닌 죽빵으로 맞으셔야 하는데 괜찮으시겠어요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 47.709,
                "end": 52.266
            },
            {
                "speaker_id": "2",
                "transcription": "이 새끼가 미쳤나 죽빵은 무슨",
                "sound_type": [
                    2,
                    3
                ],
                "start": 52.276,
                "end": 54.170
            },
            {
                "speaker_id": "3",
                "transcription": "그럼 뭐 여기서 탈락하시던가요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 54.180,
                "end": 56.398
            },
            {
                "speaker_id": "2",
                "transcription": "맞겠다고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 56.408,
                "end": 57.181
            },
            {
                "speaker_id": "2",
                "transcription": "맞을 테니까 카바니 등번호부터",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 57.191,
                "end": 59.347
            },
            {
                "speaker_id": "3",
                "transcription": "카바니 선수는 21번입니다.",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 59.357,
                "end": 61.411
            },
            {
                "speaker_id": "2",
                "transcription": "아우 뒤지게 아프네.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 61.421,
                "end": 62.707
            },
            {
                "speaker_id": "2",
                "transcription": "그럼 21에서 14를 빼면 뭐 뭐야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 62.717,
                "end": 65.385
            },
            {
                "speaker_id": "2",
                "transcription": "7번",
                "sound_type": [
                    2,
                    3
                ],
                "start": 65.395,
                "end": 65.959
            },
            {
                "speaker_id": "2",
                "transcription": "정답 크리스티아누 호날두",
                "sound_type": [
                    2,
                    3
                ],
                "start": 65.969,
                "end": 67.656
            },
            {
                "speaker_id": "1",
                "transcription": "호날두 후보 정답입니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 67.666,
                "end": 69.300
            }
        ]
    },
    "summary": "사회자가 호날두한테 정답을 말씀해 달라고 하자 호날두는 앙토니 마딸, 맥도날드라고 답해서 틀렸고 다른 선수한테 죽빵을 맞고 나서야 카바니 선수의 등번호를 알아내 크리스티아누 호날두라고 답해서 정답을 맞혔다.",
