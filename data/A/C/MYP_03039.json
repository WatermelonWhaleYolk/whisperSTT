        "term": [
            {
                "speaker_id": "1",
                "transcription": "뭐 별건 아니고 장사 비법인데.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 1.919
            },
            {
                "speaker_id": "1",
                "transcription": "사장님 스스로도 잘못한 게 있다고 느끼시는 거 맞죠?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.929,
                "end": 4.643
            },
            {
                "speaker_id": "2",
                "transcription": "아유 그럼요 제가 죄인입니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.653,
                "end": 6.509
            },
            {
                "speaker_id": "2",
                "transcription": "제가 대가리라도 박을까요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 6.519,
                "end": 7.681
            },
            {
                "speaker_id": "1",
                "transcription": "뭐 반성 한다니까 일단 여기 사인해요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.691,
                "end": 10.279
            },
            {
                "speaker_id": "1",
                "transcription": "아 이게 한국어라 읽기 힘드려나?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 10.289,
                "end": 12.171
            },
            {
                "speaker_id": "1",
                "transcription": "그럼 번역을.",
