        "term": [
            {
                "speaker_id": "1",
                "transcription": "돼지 특유의 잡내를 잡아주고요.",
                "sound_type": [
                    2
                ],
                "start": 0.000,
                "end": 1.686
            },
            {
                "speaker_id": "1",
                "transcription": "아 드시면 또 연육 작용으로 부드러운 육질을 느낄 수 있어요.",
                "sound_type": [
                    2
                ],
