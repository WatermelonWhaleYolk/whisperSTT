        "term": [
            {
                "speaker_id": "1",
                "transcription": "너 그 학교 붙었냐?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 1.420
            },
            {
                "speaker_id": "1",
                "transcription": "그럼 서울로 가네?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.430,
                "end": 3.679
            },
            {
                "speaker_id": "2",
                "transcription": "엉 가야지.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 3.689,
                "end": 5.795
            },
            {
                "speaker_id": "3",
                "transcription": "다 같이 모이기가 힘들어졌다.",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 5.805,
                "end": 7.859
            },
            {
                "speaker_id": "2",
                "transcription": "올라오면 연락해.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.869,
                "end": 9.570
            },
            {
                "speaker_id": "1",
                "transcription": "왠지 짱나.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 9.580,
                "end": 11.306
            },
            {
                "speaker_id": "1",
                "transcription": "나보다 똑똑한 놈들 모두 복수 해주마.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 11.316,
                "end": 15.099
            },
            {
                "speaker_id": "1",
                "transcription": "10년 뒤에 내 만화에 덜 나오게 해주마.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 15.109,
                "end": 18.523
            },
            {
                "speaker_id": "1",
                "transcription": "실제론 안 그랬는데도 재수 없게 그려주마",
                "sound_type": [
                    2,
                    3
                ],
                "start": 18.533,
                "end": 21.716
            },
            {
                "speaker_id": "3",
                "transcription": "그러다 보니 자연스레 정기적인 모임을 도모하는 역할이 생겨나게 되었고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 21.726,
                "end": 26.702
            },
            {
                "speaker_id": "4",
                "transcription": "오늘 한잔한다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 26.712,
                "end": 27.938
            },
            {
                "speaker_id": "4",
                "transcription": "뭘 하고 있든 다 놓고 뛰어와.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 27.948,
                "end": 30.166
            },
            {
                "speaker_id": "3",
                "transcription": "그렇게 점점 모임이 조직화되자",
                "sound_type": [
                    2,
                    3
                ],
                "start": 30.176,
                "end": 32.948
            },
            {
                "speaker_id": "5",
                "transcription": "자 두당 만 원씩 걷으면 얼추 맞겠다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 32.958,
                "end": 36.686
            },
            {
                "speaker_id": "6",
                "transcription": "2차는 내 친구 가게 가자 전화해놨어.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 36.696,
                "end": 39.562
            },
            {
                "speaker_id": "7",
                "transcription": "오호 인맥",
                "sound_type": [
                    2,
                    3
                ],
                "start": 39.572,
                "end": 41.735
            },
            {
                "speaker_id": "3",
                "transcription": "역할의 필요성이 점점 다양해지며",
                "sound_type": [
