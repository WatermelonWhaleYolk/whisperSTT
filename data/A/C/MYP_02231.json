        "term": [
            {
                "speaker_id": "1",
                "transcription": "역시나 돌아왔군요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 1.036
            },
            {
                "speaker_id": "1",
                "transcription": "가봉가이 어째서 이런 선택을 한 건지 물어봐도 될까요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.046,
                "end": 4.084
            },
            {
                "speaker_id": "2",
                "transcription": "녀석들을 배신한 건 마음에 걸리지만 어쩔 수 없었다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.094,
                "end": 6.853
            },
            {
                "speaker_id": "2",
                "transcription": "이대로라면 라멜라 녀석이 최후의 1인이 될 거였다고 날 욕하려거든 해도 좋아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 6.863,
                "end": 11.484
            },
            {
                "speaker_id": "2",
                "transcription": "다만 난 아스날의 주장으로서 책임을 다 했다고 생각해.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 11.494,
                "end": 14.493
            },
            {
                "speaker_id": "1",
                "transcription": "하긴 아스날이라면 겨우 겨우 강금은 면하고 있으니까요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 14.503,
                "end": 17.140
            },
            {
                "speaker_id": "2",
                "transcription": "그 정도는 아니거든.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 17.150,
                "end": 18.089
            },
            {
                "speaker_id": "2",
                "transcription": "아무튼 당장 더보기 리그에서 탈출할 비법부터 알려 달라고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 18.099,
                "end": 21.553
            },
            {
                "speaker_id": "1",
                "transcription": "뭐 사실 제가 아스날로 이적하는 게 가장 효과적이긴 한데",
                "sound_type": [
                    2,
                    3
                ],
                "start": 21.563,
                "end": 24.310
            },
            {
                "speaker_id": "2",
                "transcription": "뭐라고?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 24.320,
                "end": 24.998
            },
            {
                "speaker_id": "1",
                "transcription": "아무튼 가봉가이 아니 이게 린가봉가이라고 불러야겠네요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 25.008,
                "end": 27.810
            },
            {
                "speaker_id": "1",
                "transcription": "코치님과 함께 곧 연락드리고 찾아뵐 테니 곧 린의 칭호를 달고 부활하실 수 있을 겁니다.",
                "sound_type": [
