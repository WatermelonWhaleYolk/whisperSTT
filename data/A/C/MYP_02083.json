        ],
        "term": [
            {
                "speaker_id": "1",
                "transcription": "저기 안녕하세요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 0.915
            },
            {
                "speaker_id": "1",
                "transcription": "마샬 선배",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.925,
                "end": 1.524
            },
            {
                "speaker_id": "2",
                "transcription": "어 넌 뭐야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.534,
                "end": 2.845
            },
            {
                "speaker_id": "1",
                "transcription": "아 저는 신입생 제이든 산초라고 합니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 2.855,
                "end": 5.221
            },
            {
                "speaker_id": "2",
                "transcription": "그래?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.231,
                "end": 5.855
            },
            {
                "speaker_id": "2",
                "transcription": "나한텐 무슨 일이지?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.865,
                "end": 6.853
            },
            {
                "speaker_id": "1",
                "transcription": "그게 카바니 선배님께 9번을 양보하셨다고 들었습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 6.863,
                "end": 9.747
            },
            {
                "speaker_id": "2",
                "transcription": "양보는 무슨 뺏긴거나 다름없다고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 9.757,
                "end": 12.045
            },
            {
                "speaker_id": "1",
                "transcription": "아 그렇군요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 12.055,
                "end": 12.829
            },
            {
                "speaker_id": "1",
                "transcription": "그럼 앞으로 몇 번을 달 예정이신지 여쭤봐도 되겠습니까?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 12.839,
                "end": 15.757
            },
            {
                "speaker_id": "2",
                "transcription": "18",
                "sound_type": [
                    2,
                    3
                ],
                "start": 15.767,
                "end": 16.310
            },
            {
                "speaker_id": "1",
                "transcription": "예?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 16.320,
                "end": 16.767
            },
            {
                "speaker_id": "1",
                "transcription": "아니 번호 좀 여쭤봤다고 갑자기 욕을 하시면",
                "sound_type": [
                    2,
                    3
                ],
                "start": 16.777,
                "end": 18.989
            },
            {
                "speaker_id": "2",
                "transcription": "18",
                "sound_type": [
                    2,
                    3
                ],
                "start": 18.999,
                "end": 19.648
            },
            {
                "speaker_id": "1",
                "transcription": "이보세요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 19.658,
                "end": 20.079
            },
            {
                "speaker_id": "1",
                "transcription": "마샬 선배 실력만큼 입버릇도 엉망이신 겁니까?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 20.089,
                "end": 22.560
            },
            {
                "speaker_id": "1",
                "transcription": "듣던 대로 진짜 별로시네요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 22.570,
                "end": 24.129
            },
            {
                "speaker_id": "2",
                "transcription": "18번이라고 이 답답한 자식아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 24.139,
                "end": 25.954
            },
            {
                "speaker_id": "1",
                "transcription": "답도 없는 새끼라고요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 25.964,
                "end": 27.473
            },
            {
                "speaker_id": "1",
                "transcription": "진짜 입에 걸레를 무셨나?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 27.483,
                "end": 28.615
            },
            {
                "speaker_id": "2",
                "transcription": "내가 답답한 자식이랬지 언제 답도 없는 새끼라 그랬어?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 28.625,
                "end": 31.547
            },
            {
                "speaker_id": "2",
                "transcription": "난 9번을 넘겨주고 18번을 달고 있다니까 자꾸 무슨 소릴 하는 거야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 31.557,
                "end": 35.451
            },
            {
                "speaker_id": "1",
                "transcription": "아 죄송합니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 35.461,
                "end": 36.454
            },
            {
                "speaker_id": "1",
                "transcription": "제가 오해를 했나 봅니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 36.464,
                "end": 37.607
            },
            {
                "speaker_id": "3",
                "transcription": "어이 산초 드디어 맨유에 입성했구나.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 37.617,
                "end": 39.943
            },
            {
                "speaker_id": "1",
                "transcription": "린가드 선배 이게 얼마만이에요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 39.953,
                "end": 41.685
            },
            {
                "speaker_id": "3",
                "transcription": "그래 그래 반갑다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 41.695,
                "end": 42.600
            },
            {
                "speaker_id": "3",
                "transcription": "근데 마샬 너는 하라는 훈련은 안 하고 신입생을 괴롭히고 있어?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 42.610,
                "end": 45.970
            },
            {
                "speaker_id": "3",
                "transcription": "그러니까 9급으로 떨어지는 거라고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 45.980,
                "end": 47.548
            },
            {
                "speaker_id": "2",
                "transcription": "무슨 소리야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 47.558,
                "end": 48.315
            },
            {
                "speaker_id": "2",
                "transcription": "오히려 이 녀석이 멋대로 오해해서 날 괴롭히고 있었다고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 48.325,
                "end": 51.320
            },
            {
                "speaker_id": "3",
                "transcription": "그래?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 51.330,
                "end": 51.935
            },
            {
                "speaker_id": "3",
                "transcription": "아이 그러니까 그 표정 좀 풀고 다니라고 멀리서 보고 너가 괴롭힌 줄 알았잖아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 51.945,
                "end": 55.788
            },
            {
                "speaker_id": "2",
                "transcription": "내 표정이 어때서?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 55.798,
                "end": 57.122
            },
            {
                "speaker_id": "3",
                "transcription": "어떻긴 아주 그냥",
                "sound_type": [
                    2,
                    3
                ],
                "start": 57.132,
                "end": 58.215
            },
            {
                "speaker_id": "1",
                "transcription": "아이 싸우지들 마세요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 58.225,
                "end": 59.231
            },
            {
                "speaker_id": "1",
                "transcription": "제가 멋대로 오해하는 바람에 죄송합니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 59.241,
                "end": 62.233
            }
        ]
    },
    "summary": "맨유에 새로 영입된 산초 선수가 마샬 선수 등번호를 물어봤는데 욕으로 오해하는 바람에 마샬 선수와 린가드 선수가 싸우게 됐다.",
