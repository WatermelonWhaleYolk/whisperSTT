        "term": [
            {
                "speaker_id": "1",
                "transcription": "누가 재수없게 울고 지랄이야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 4.206
            },
            {
                "speaker_id": "2",
                "transcription": "나다 이 새끼야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.216,
                "end": 5.536
            },
            {
                "speaker_id": "3",
                "transcription": "오 오태식이",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.546,
                "end": 7.137
            },
            {
                "speaker_id": "4",
                "transcription": "오 오태식이 돌아왔구나.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.147,
                "end": 9.378
            },
            {
                "speaker_id": "4",
                "transcription": "오디션 얘기는 들었다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 9.388,
                "end": 10.919
            },
            {
                "speaker_id": "4",
                "transcription": "아 보아하니 또 잘 안 됐나 보구나.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 10.929,
                "end": 13.080
            },
            {
                "speaker_id": "3",
                "transcription": "쟤는 오태식 우리 팀 오른쪽 수비수다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 13.090,
                "end": 15.799
            },
            {
                "speaker_id": "3",
                "transcription": "타랍박 너와 같은 라인에서 호흡을 맞춰야 하니까 친하게 지내라.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 15.809,
                "end": 19.905
            },
            {
                "speaker_id": "2",
                "transcription": "꼭 그렇게 다 떨어트려야만 속이 후련했냐.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 19.915,
                "end": 24.269
            },
            {
                "speaker_id": "1",
                "transcription": "아니 언제 봤다고 갑자기 성질이야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 24.279,
                "end": 25.924
            },
            {
                "speaker_id": "3",
                "transcription": "오태식이는 배우 지망생이다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 25.934,
                "end": 27.887
            },
            {
                "speaker_id": "3",
                "transcription": "근데 뭐 잘생기긴 했지만 사실 좀 옛날 얼굴이다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 27.897,
                "end": 31.370
            },
            {
                "speaker_id": "1",
                "transcription": "뭐 좀옛날 얼굴이긴 하네요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 31.380,
                "end": 32.937
            },
            {
                "speaker_id": "1",
                "transcription": "요즘은 뭐랄까 저같은 얼굴이 대세죠.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 32.947,
                "end": 35.637
