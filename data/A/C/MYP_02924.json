        "term": [
            {
                "speaker_id": "1",
                "transcription": "아니 무슨 합격 목걸이가 레드 카드야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 2.821
            },
            {
                "speaker_id": "2",
                "transcription": "아니 법사님 그러니까 저를 사용 하셨어야죠.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 2.831,
                "end": 5.860
            },
            {
                "speaker_id": "1",
                "transcription": "네놈은 닥치고 있거라.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.870,
                "end": 7.894
            },
            {
                "speaker_id": "2",
                "transcription": "아니 쓰지도 않을 거면서 왜 데려오셨습니까?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.904,
                "end": 10.385
            },
            {
                "speaker_id": "1",
                "transcription": "여기 안 데려왔으면 골프장에 가있을 거 다 알고 있다. 이놈아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 10.395,
                "end": 14.822
            },
            {
                "speaker_id": "3",
                "transcription": "상대로 안 되는 것들이 지들끼리 싸우고 앉아있군.",
