            },
            {
                "speaker_id": "2",
                "transcription": "아니 그래도 그렇죠.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 3.271,
                "end": 4.131
            },
            {
                "speaker_id": "2",
                "transcription": "라인업에도 없으신 분이 나와 버리면",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.141,
                "end": 5.877
