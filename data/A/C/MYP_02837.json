        "term": [
            {
                "speaker_id": "1",
                "transcription": "제가 그 쪽으로 가겠어요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 2.263
            },
            {
                "speaker_id": "2",
                "transcription": "아니 뭐야 저건 또 이니스타 아니 로보트인가?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 2.273,
                "end": 5.400
            },
            {
                "speaker_id": "3",
                "transcription": "그래 기가지니에스타야 휴가는 즐거웠니?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.410,
                "end": 8.072
            },
            {
                "speaker_id": "1",
                "transcription": "휴가는 개꿀맛이었어요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 8.082,
                "end": 10.468
            },
            {
                "speaker_id": "1",
                "transcription": "감독님도 잘 지내셨죠?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 10.478,
                "end": 12.711
            },
            {
                "speaker_id": "3",
                "transcription": "그래 그래 인사해라 타랍박.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 12.721,
                "end": 14.826
            },
            {
                "speaker_id": "3",
                "transcription": "이 친구 아니 이 인공지능 로봇이 우리 팀 중앙 미드필더 기가지니에스타야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 14.836,
                "end": 19.750
            },
            {
                "speaker_id": "2",
                "transcription": "아니 무슨 이제 하다 하다 팀에 로봇도 있어요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 19.760,
                "end": 22.396
            },
            {
                "speaker_id": "1",
                "transcription": "너무해요 타랍박.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 22.406,
                "end": 24.102
            },
            {
                "speaker_id": "1",
                "transcription": "로봇도 마음이 아파요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 24.112,
                "end": 26.026
            },
            {
                "speaker_id": "2",
                "transcription": "아니 감독님이랑 얘기 중이니까 조용히 좀 해요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 26.036,
                "end": 28.469
            },
            {
                "speaker_id": "1",
                "transcription": "조용필 노래를 재생합니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 28.479,
                "end": 30.631
            },
            {
                "speaker_id": "1",
                "transcription": "그대가 돌아오면 두 눈이 마주칠까 심장이 바운스 바운스 바운스 돼 들킬까 봐 겁나",
                "sound_type": [
                    2,
                    3
                ],
                "start": 30.641,
                "end": 37.663
            },
            {
                "speaker_id": "2",
                "transcription": "아니 뭐 이딴 게 다 있어 어이 기가지니에스타 나는 바보입니다 해봐.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 37.673,
                "end": 41.986
            },
            {
                "speaker_id": "1",
                "transcription": "잘 못 알아들었어요.",
