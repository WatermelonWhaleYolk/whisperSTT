        "term": [
            {
                "speaker_id": "1",
                "transcription": "우리는 방관할 수 없었다.",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 0.000,
                "end": 3.776
            },
            {
                "speaker_id": "2",
                "transcription": "이야 회장님 단물 쏟으신다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 3.786,
                "end": 6.445
            },
            {
                "speaker_id": "3",
                "transcription": "뭐하냐 등 안 두드리고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 6.455,
                "end": 7.896
            },
            {
                "speaker_id": "4",
                "transcription": "성스러운 순간들을 함께 하자.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.906,
                "end": 9.911
            },
            {
                "speaker_id": "1",
                "transcription": "그때 찍은 이 사진은 우리 카페의 대문이 되었고 취한 그가 길가에 쓰러지지 않으려다",
                "sound_type": [
                    2,
                    3
                ],
                "start": 9.921,
                "end": 18.279
            },
            {
                "speaker_id": "5",
                "transcription": "뭐야 회장님 넘어간다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 18.289,
                "end": 19.837
            },
            {
                "speaker_id": "6",
                "transcription": "자 잡아 잡아",
                "sound_type": [
                    2,
                    3
                ],
                "start": 19.847,
                "end": 21.437
            },
            {
