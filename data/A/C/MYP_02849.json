        "term": [
            {
                "speaker_id": "1",
                "transcription": "자 시청자 여러분 안녕하십니까?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 1.363
            },
            {
                "speaker_id": "1",
                "transcription": "2018 아시안게임 한국과 일본의 결승전이 펼쳐지고 있습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.373,
                "end": 5.292
            },
            {
                "speaker_id": "1",
                "transcription": "전후반 내내 골이 나오지 않은 상황에서 연장전으로 돌입하고 있는 상황입니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.302,
                "end": 8.958
            },
            {
                "speaker_id": "2",
                "transcription": "아 이대로라면 군대각이다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 8.968,
                "end": 10.720
            },
            {
                "speaker_id": "2",
                "transcription": "잘있어 토트넘.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 10.730,
                "end": 11.622
            },
            {
                "speaker_id": "2",
                "transcription": "상추상무로 간다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 11.632,
                "end": 12.643
            },
            {
                "speaker_id": "3",
                "transcription": "손흥민 씨 손흥민 씨",
                "sound_type": [
                    2,
                    3
                ],
                "start": 12.653,
                "end": 14.725
            },
            {
                "speaker_id": "2",
                "transcription": "어 의 의조야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 14.735,
                "end": 15.986
            },
            {
                "speaker_id": "3",
                "transcription": "손흥민 씨 정신 차리세요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 15.996,
                "end": 17.270
            },
            {
                "speaker_id": "3",
                "transcription": "당신 아직 군인 아닙니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 17.280,
                "end": 18.419
            },
            {
                "speaker_id": "4",
                "transcription": "귀인이에요 귀인.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 18.429,
                "end": 19.467
            },
            {
                "speaker_id": "4",
                "transcription": "귀인이 나타났어요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 19.477,
                "end": 20.721
            },
            {
                "speaker_id": "2",
                "transcription": "스 승우야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 20.731,
                "end": 21.659
            },
            {
                "speaker_id": "1",
                "transcription": "자 이승우 패널티에서 공을 잡습니다. 그대로 슛 골",
                "sound_type": [
                    2,
                    3
                ],
                "start": 21.669,
                "end": 24.803
            },
            {
                "speaker_id": "4",
                "transcription": "형 어디 제 허락도 없이 입대 하시려고 그러세요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 24.813,
                "end": 27.693
            },
            {
                "speaker_id": "2",
                "transcription": "아 아니 너가 어떻게 골을",
                "sound_type": [
                    2,
                    3
                ],
                "start": 27.703,
                "end": 28.969
            },
            {
                "speaker_id": "5",
                "transcription": "여기 귀인 한 명 추가요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 28.979,
                "end": 30.892
            },
            {
                "speaker_id": "2",
                "transcription": "희 희찬아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 30.902,
                "end": 31.483
            },
            {
                "speaker_id": "1",
                "transcription": "아 골문 앞에서 황희찬이 헤더 골 들어갑니다.",
