        "term": [
            {
                "speaker_id": "1",
                "transcription": "아 예 지금 다시 보시니까 남다르실 거 같아요.",
                "sound_type": [
                    2
                ],
                "start": 0.000,
                "end": 3.829
            },
            {
                "speaker_id": "1",
                "transcription": "다음 화면도 한번 볼까요 저희가?",
