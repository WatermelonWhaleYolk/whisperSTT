        "term": [
            {
                "speaker_id": "1",
                "transcription": "주문 밀려서 좀 기다리셔야 될 텐데",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 2.137
            },
            {
                "speaker_id": "1",
                "transcription": "어 어서오세요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 2.147,
                "end": 3.270
            },
            {
                "speaker_id": "1",
                "transcription": "주문하시겠어요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 3.280,
                "end": 4.824
            },
            {
                "speaker_id": "2",
                "transcription": "피자 10판 다 됐나?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.834,
                "end": 7.636
            },
            {
                "speaker_id": "1",
                "transcription": "진짜 좀 이따 왔네.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.646,
                "end": 8.669
            },
            {
                "speaker_id": "1",
                "transcription": "벌집 피자 말씀하시는 건가?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 8.679,
                "end": 10.783
            },
            {
                "speaker_id": "3",
                "transcription": "그날은 역시 뭔가 이상했다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 10.793,
                "end": 13.937
            },
            {
                "speaker_id": "3",
                "transcription": "그 손님을 시작으로",
                "sound_type": [
                    2,
                    3
                ],
                "start": 13.947,
                "end": 15.464
            },
            {
                "speaker_id": "4",
                "transcription": "뭘 피자가 그거 대충 고명 좀 올려서 렌지 돌리면 되지.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 15.474,
                "end": 18.719
            },
            {
                "speaker_id": "4",
                "transcription": "우리 마누라 음식 전부 그렇게 만드는 것 같드만",
                "sound_type": [
                    2,
                    3
                ],
                "start": 18.729,
                "end": 21.375
            },
            {
                "speaker_id": "4",
                "transcription": "내 피자 달라고 내 놓으라고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 21.385,
                "end": 24.020
            },
            {
                "speaker_id": "1",
                "transcription": "잠시만 기다려주시면",
                "sound_type": [
                    2,
                    3
                ],
                "start": 24.030,
                "end": 25.286
            },
            {
                "speaker_id": "5",
                "transcription": "피자 되죠?",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 25.296,
                "end": 25.832
            },
            {
                "speaker_id": "1",
                "transcription": "어",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 25.842,
                "end": 26.850
            },
            {
                "speaker_id": "3",
                "transcription": "갑자기 손님들이 물 밀듯이 몰려들기 시작했는데",
                "sound_type": [
                    2,
                    3
                ],
                "start": 26.860,
                "end": 31.069
            },
            {
                "speaker_id": "5",
                "transcription": "뭐가 제일 맛있어요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 31.079,
                "end": 32.289
            },
            {
                "speaker_id": "4",
                "transcription": "오래 걸리나?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 32.299,
                "end": 33.035
            },
            {
                "speaker_id": "1",
                "transcription": "아 저기 천천",
                "sound_type": [
                    2,
                    3
                ],
                "start": 33.045,
                "end": 34.791
            },
            {
                "speaker_id": "1",
                "transcription": "이게 무슨 일이야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 34.801,
                "end": 36.119
            },
            {
                "speaker_id": "3",
                "transcription": "주말이나 휴일도 아닌데 유례 없던 러쉬가 몰리자",
                "sound_type": [
                    2,
                    3
                ],
