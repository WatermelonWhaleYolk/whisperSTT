        "term": [
            {
                "speaker_id": "1",
                "transcription": "어 얘가 그 새 어딜 갔어?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 1.690
            },
            {
                "speaker_id": "2",
                "transcription": "흥민이 녀석 그새를 못 참고 뒷공간 침투하러 갔나 본데요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.700,
                "end": 5.321
            },
            {
                "speaker_id": "1",
                "transcription": "아니 아직 시작도 안 했잖아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.331,
                "end": 6.725
            },
            {
                "speaker_id": "1",
                "transcription": "어찌 됐든 베일 너도",
                "sound_type": [
                    2,
                    3
                ],
                "start": 6.735,
                "end": 7.942
            },
            {
                "speaker_id": "2",
                "transcription": "저기 감독님 지난 경기에 무리했더니 오늘은 무릎이 좀.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.952,
                "end": 11.003
            },
            {
                "speaker_id": "1",
                "transcription": "너는 진짜 1경기 1병원도 아니고 됐다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 11.013,
                "end": 14.297
            },
            {
                "speaker_id": "1",
                "transcription": "다음으로 은돔벨레",
                "sound_type": [
                    2,
                    3
                ],
                "start": 14.307,
                "end": 15.328
            },
            {
                "speaker_id": "3",
                "transcription": "리버풀 잡기 딱 좋은 날씨 리버풀 잡기 딱 좋은 날씨.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 15.338,
                "end": 21.578
            },
            {
                "speaker_id": "4",
                "transcription": "아니 저건 은돔벨레가 참돔벨레 모드를 발동할 때 부른다는 전설의 노래?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 21.588,
                "end": 26.151
            },
            {
                "speaker_id": "2",
                "transcription": "에이 아니지 저건 돌돔벨레 모드 발동할 때 부르는 거잖아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 26.161,
                "end": 29.330
            },
            {
                "speaker_id": "1",
                "transcription": "뭐 뭐야 나만 뭔 소린지 모르는 거야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 29.340,
                "end": 31.584
            },
            {
                "speaker_id": "1",
                "transcription": "아무튼 창의적인 패스는 네 발끝에 달려 있다.",
