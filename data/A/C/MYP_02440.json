        "term": [
            {
                "speaker_id": "1",
                "transcription": "아무튼 니들도 들었을 거 아니야?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 1.548
            },
            {
                "speaker_id": "1",
                "transcription": "우리 스승님이 새 팀을 맡으셨다고.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.558,
                "end": 3.677
            },
            {
                "speaker_id": "2",
                "transcription": "스승님이라면 아 포체티노 감독님?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 3.687,
                "end": 5.759
            },
            {
                "speaker_id": "1",
                "transcription": "그래 솔직히 토트넘이니 인테르니 지금 우리 데스크 라인이 흩어져 있지만 파리에서 다 같이 뭉치는 거 어때?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.769,
                "end": 11.459
            },
            {
                "speaker_id": "3",
                "transcription": "야 파리가 뉘 집 개 이름이냐?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 11.469,
                "end": 13.467
            },
            {
                "speaker_id": "3",
                "transcription": "거기 가면 음바페, 네이마르랑 경쟁해야 해.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 13.477,
                "end": 15.845
            },
            {
                "speaker_id": "1",
                "transcription": "아니 그래도 다 모이면 다시 예전처럼 잘해질 것 같다니까.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 15.855,
                "end": 18.916
            },
            {
                "speaker_id": "2",
                "transcription": "에이 아무리 그래도 네이마르, 음바페를 어떻게 이겨.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 18.926,
                "end": 21.332
            },
            {
                "speaker_id": "2",
                "transcription": "그리고 난 토트넘에서 우승을",
                "sound_type": [
                    2,
                    3
                ],
                "start": 21.342,
                "end": 22.460
            },
            {
                "speaker_id": "3",
                "transcription": "정신 차려 손흥민 토트넘이 우승을 한다고?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 22.470,
                "end": 25.200
            }
        ]
    },
