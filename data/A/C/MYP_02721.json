        "term": [
            {
                "speaker_id": "1",
                "transcription": "계십니까?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 0.464
            },
            {
                "speaker_id": "2",
                "transcription": "네 어떻게 오셨습니까?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.474,
                "end": 1.708
            },
            {
                "speaker_id": "1",
                "transcription": "아 저는 토트넘 핫스퍼의 회장 다니엘 레비입니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.718,
                "end": 4.619
            },
            {
                "speaker_id": "1",
                "transcription": "선수 좀 영입하러 왔구요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.629,
                "end": 6.123
            },
            {
                "speaker_id": "2",
                "transcription": "이 사람이 지금 장난하나?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 6.133,
                "end": 7.192
            },
            {
                "speaker_id": "2",
                "transcription": "토트넘이면 프리미어리그잖아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.202,
                "end": 8.977
            },
            {
                "speaker_id": "2",
                "transcription": "여기는 어디 김치 유나이티드나 그런 잠깐 저 사람 무리뉴 감독?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 8.987,
                "end": 13.188
            },
            {
                "speaker_id": "2",
                "transcription": "이것들 진짜 토트넘인가 본데 하긴 뭐 토트넘이 짠돌이로 유명하긴 하지.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 13.198,
                "end": 17.218
            },
            {
                "speaker_id": "2",
                "transcription": "이봐요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 17.228,
                "end": 17.914
            },
            {
                "speaker_id": "3",
                "transcription": "진짜 이런 데 괜찮은 선수가 있어요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 17.924,
                "end": 19.732
            },
            {
                "speaker_id": "2",
                "transcription": "이거 이거 잘하면 호구 잡아서 한몫 챙기겠는데",
                "sound_type": [
                    2,
                    3
                ],
                "start": 19.742,
                "end": 22.233
            },
            {
                "speaker_id": "2",
                "transcription": "아 아이 그럼요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 22.243,
                "end": 23.437
            },
            {
                "speaker_id": "2",
                "transcription": "괜찮은 선수가 몇 명 있습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 23.447,
                "end": 24.588
            },
            {
                "speaker_id": "2",
                "transcription": "이쪽으로 오시죠.",
