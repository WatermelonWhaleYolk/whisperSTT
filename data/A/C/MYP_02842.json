        "term": [
            {
                "speaker_id": "1",
                "transcription": "자 여기가 머지사이드 최고의 삼겹살집 훔초갈비다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 4.122
            },
            {
                "speaker_id": "2",
                "transcription": "아니 삼겹살이 다 거기서 거기지 뭐라고 이렇게 난리야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.132,
                "end": 6.981
            },
            {
                "speaker_id": "2",
                "transcription": "뭐 냉동 삼겹살이 1인분에 15파운드 한국 돈으로 치면 2만 원이 넘잖아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 6.991,
                "end": 12.063
            },
            {
                "speaker_id": "2",
                "transcription": "거기다가 1인분에 고작 100그람이라고",
                "sound_type": [
                    2,
                    3
                ],
                "start": 12.073,
                "end": 14.446
            },
            {
                "speaker_id": "3",
                "transcription": "타랍박 한국 음식 많이 그리웠지?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 14.456,
                "end": 16.681
            },
            {
                "speaker_id": "3",
                "transcription": "여기 사장님도 한국분이다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 16.691,
                "end": 18.525
            },
            {
                "speaker_id": "3",
                "transcription": "많이 먹어라.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 18.535,
                "end": 19.683
            },
            {
                "speaker_id": "4",
                "transcription": "아이고 뭐 김치공장에서 회식 왔나?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 19.693,
                "end": 22.177
            },
            {
                "speaker_id": "4",
                "transcription": "어라 못 보던 친군데 마 니 한국인이가?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 22.187,
                "end": 25.289
            },
            {
                "speaker_id": "2",
                "transcription": "아 예 안녕하세요? 뭐야",
