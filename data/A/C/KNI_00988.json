        "term": [
            {
                "speaker_id": "1",
                "transcription": "레오가 공이 발에 걸리자 관중석을 향해 그대로 걷어찹니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 4.111
            },
            {
                "speaker_id": "1",
                "transcription": "레드카드를 받고 우리 카드에 1점과 서브권을 내주자 석진욱 감독이 흥분을 감추지 못합니다.",
                "sound_type": [
                    2,
                    4
                ],
                "start": 4.121,
                "end": 12.133
            },
            {
                "speaker_id": "2",
                "transcription": "알렉스가 차면 레드카드 안 주고 얘가 차면 레드카드를 줘요?",
                "sound_type": [
                    2,
                    4
                ],
                "start": 12.143,
                "end": 14.776
            },
            {
                "speaker_id": "3",
                "transcription": "아니 그건 지나간 경기이고",
                "sound_type": [
                    2,
                    4
                ],
                "start": 14.786,
                "end": 16.071
            },
            {
                "speaker_id": "2",
                "transcription": "아니 지나갔으니까 그거로 맞춰서 가야죠.",
                "sound_type": [
                    2,
                    4
                ],
                "start": 16.081,
                "end": 18.408
            },
            {
                "speaker_id": "3",
                "transcription": "아니요 아니요 아니요.",
                "sound_type": [
                    2,
                    4
                ],
                "start": 18.418,
                "end": 19.268
            },
            {
                "speaker_id": "2",
                "transcription": "지금부터 왜 그래요?",
                "sound_type": [
                    2,
                    4
                ],
                "start": 19.278,
                "end": 20.459
            },
            {
                "speaker_id": "3",
                "transcription": "그때 잘못 준 거예요.",
