            {
                "speaker_id": "2",
                "transcription": "네 알겠습니다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.805,
                "end": 5.631
            },
            {
                "speaker_id": "2",
                "transcription": "일단 레시피도 줘야 되는 거 아니에요?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.641,
                "end": 7.052
            },
            {
                "speaker_id": "2",
                "transcription": "레시피 갖고 와요 빨리.",
