        "term": [
            {
                "speaker_id": "1",
                "transcription": "아시죠?",
                "sound_type": [
                    2
                ],
                "start": 0.000,
                "end": 1.443
            },
            {
                "speaker_id": "2",
                "transcription": "별로 축구에는 관심이 없네요 친구는?",
                "sound_type": [
                    2
                ],
                "start": 1.453,
                "end": 4.068
            },
            {
                "speaker_id": "1",
                "transcription": "축구에 관심이 있어요.",
                "sound_type": [
                    2
                ],
                "start": 4.078,
                "end": 5.575
            },
            {
                "speaker_id": "2",
                "transcription": "없는 거 같은데 꾸미는 거만",
                "sound_type": [
                    2
                ],
                "start": 5.585,
                "end": 8.145
            },
            {
                "speaker_id": "1",
                "transcription": "머리 스타일 일단 제 헤어스타일로 상대방 기선 제압하고요.",
                "sound_type": [
                    2
                ],
                "start": 8.155,
                "end": 14.290
            },
            {
                "speaker_id": "1",
                "transcription": "일단 제 헤어스타일 때문에 상대들이 저 프로 선수인 줄 알겠죠?",
                "sound_type": [
                    2
                ],
                "start": 14.300,
                "end": 17.998
            },
            {
                "speaker_id": "1",
                "transcription": "그만큼 걔들이 못 뛰겠죠?",
                "sound_type": [
                    2
                ],
                "start": 18.008,
                "end": 21.520
            },
            {
                "speaker_id": "3",
                "transcription": "천성적으로 긴장이 먼 @이름1과 정반대 성격의 친구가 바로 아나스입니다.",
                "sound_type": [
                    2
                ],
                "start": 21.530,
                "end": 28.308
            },
            {
                "speaker_id": "4",
                "transcription": "나스가 골대만 안 맞히면 이겨 나스.",
                "sound_type": [
                    2
                ],
                "start": 28.318,
                "end": 30.827
            },
            {
                "speaker_id": "5",
                "transcription": "컨디션은?",
                "sound_type": [
                    2
                ],
                "start": 30.837,
                "end": 32.642
            },
            {
                "speaker_id": "6",
                "transcription": "배가 좀 아프긴 한데 괜찮아요.",
                "sound_type": [
                    2
                ],
                "start": 32.652,
                "end": 35.927
            },
            {
                "speaker_id": "5",
                "transcription": "긴장돼?",
                "sound_type": [
                    2
                ],
                "start": 35.937,
                "end": 37.730
            },
            {
                "speaker_id": "6",
                "transcription": "되게 긴장돼?",
                "sound_type": [
                    2
                ],
                "start": 37.740,
                "end": 39.116
            },
            {
                "speaker_id": "4",
                "transcription": "아니.",
                "sound_type": [
                    2
                ],
                "start": 39.126,
                "end": 39.868
            },
            {
                "speaker_id": "6",
                "transcription": "별로 안 긴장돼요.",
                "sound_type": [
                    2
                ],
                "start": 39.878,
                "end": 40.925
            },
            {
                "speaker_id": "4",
                "transcription": "나스 긴장돼 설마?",
                "sound_type": [
                    2
                ],
                "start": 40.935,
                "end": 42.300
            },
            {
                "speaker_id": "7",
                "transcription": "나스하고 야 나스, 세바스찬 컴온.",
