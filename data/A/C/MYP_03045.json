        ],
        "term": [
            {
                "speaker_id": "1",
                "transcription": "그래도 파티엔 꼭 오세요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 1.215
            },
            {
                "speaker_id": "2",
                "transcription": "이 안 가.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 1.225,
                "end": 2.217
            },
            {
                "speaker_id": "1",
                "transcription": "말씀은 저렇게 하셔도 꼭 오실 거야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 2.227,
                "end": 4.120
            },
            {
                "speaker_id": "3",
                "transcription": "아 예.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.120,
                "end": 4.720
            },
            {
                "speaker_id": "2",
                "transcription": "끼야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 4.730,
                "end": 7.955
            },
            {
                "speaker_id": "2",
                "transcription": "이 이 희끄무레 한 건 뭐야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.965,
                "end": 10.278
            },
            {
                "speaker_id": "4",
                "transcription": "난 과거의 유령인데.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 10.288,
                "end": 11.983
            },
            {
                "speaker_id": "4",
                "transcription": "그러니까 따라와.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 11.993,
                "end": 13.560
            },
            {
                "speaker_id": "2",
                "transcription": "아이 이 이거 못 놔.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 13.570,
                "end": 15.859
            },
            {
                "speaker_id": "2",
                "transcription": "아이 저건 나잖아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 15.869,
                "end": 19.764
            },
            {
                "speaker_id": "4",
                "transcription": "맞아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 19.774,
                "end": 20.475
            },
            {
                "speaker_id": "5",
                "transcription": "이 다 이리들 모여봐라.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 20.485,
                "end": 21.621
            },
            {
                "speaker_id": "5",
                "transcription": "이 감독이 누누이 말하지 않았느냐.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 21.631,
                "end": 23.240
            },
            {
                "speaker_id": "5",
                "transcription": "경기에서 이기는 것도 중요하지만 팀플레이를 하란 말이야 팀플레이를.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 23.250,
                "end": 26.675
            },
            {
                "speaker_id": "5",
                "transcription": "호크루지 자네가 양보만 했어도 지성이랑 루니도 골을 넣을 수 있었어.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 26.685,
                "end": 30.232
            },
            {
                "speaker_id": "6",
                "transcription": "아니 뭐 제가 잘하는 걸 어쩌라고요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 30.228,
                "end": 31.811
            },
            {
                "speaker_id": "6",
                "transcription": "감독님은 맨날 저한테만 뭐라 하시고.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 31.821,
                "end": 33.505
            },
            {
                "speaker_id": "6",
                "transcription": "저 마드리드로 떠날 거예요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 33.515,
                "end": 34.751
            },
            {
                "speaker_id": "4",
                "transcription": "감독님도 좋은 뜻으로 말했을 텐데.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 34.761,
                "end": 36.639
            },
            {
                "speaker_id": "4",
                "transcription": "너무 한 거 아니냐?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 36.649,
                "end": 37.630
            },
            {
                "speaker_id": "2",
                "transcription": "무슨 소리야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 37.640,
                "end": 38.744
            },
            {
                "speaker_id": "2",
                "transcription": "내가 맨유에서 넣어준 골이 몇 갠데.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 38.754,
                "end": 41.634
            },
            {
                "speaker_id": "2",
                "transcription": "그래도 퍼거슨 감독님 참 좋은 분이셨지.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 41.644,
                "end": 44.690
            },
            {
                "speaker_id": "2",
                "transcription": "지성이한테 패스라도 몇 번 더 할 걸 그랬어.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 44.700,
                "end": 47.825
            },
            {
                "speaker_id": "2",
                "transcription": "지 지성아 패 패스받아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 47.835,
                "end": 51.082
            },
            {
                "speaker_id": "2",
                "transcription": "지 지성아.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 51.092,
                "end": 52.323
            },
            {
                "speaker_id": "2",
                "transcription": "아니 넌 또 뭐야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 52.333,
                "end": 55.811
            },
            {
                "speaker_id": "7",
                "transcription": "나는 현재의 유령이다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 55.821,
                "end": 57.272
            },
            {
                "speaker_id": "7",
                "transcription": "일단 따라와.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 57.282,
                "end": 58.027
            },
            {
                "speaker_id": "7",
                "transcription": "가자.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 58.037,
                "end": 58.484
            },
            {
                "speaker_id": "2",
                "transcription": "또 또 어디 간다는 게야.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 58.484,
                "end": 62.366
            }
        ]
    },
    "summary": "호크루지가 과거의 유령을 따라가 지성이에게 패스를 좀 더 해줄걸 후회한다.",
