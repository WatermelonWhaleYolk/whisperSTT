        "term": [
            {
                "speaker_id": "1",
                "transcription": "리버풀 파울 맨체스터 유나이티드 페널티킥",
                "sound_type": [
                    2
                ],
                "start": 0.000,
                "end": 2.231
            },
            {
                "speaker_id": "2",
                "transcription": "뭐라고요 이번엔 또 뭔데요?",
                "sound_type": [
                    2
                ],
                "start": 2.241,
                "end": 3.930
            },
            {
                "speaker_id": "1",
                "transcription": "로버트슨이 포그바를 밀었잖아 명백한 페널티킥이다.",
                "sound_type": [
                    2
                ],
                "start": 3.940,
                "end": 6.637
            },
            {
                "speaker_id": "3",
                "transcription": "자꾸 뭔 소리를 하시는 겁니까?",
                "sound_type": [
                    2
                ],
                "start": 6.647,
                "end": 7.914
            },
            {
                "speaker_id": "3",
                "transcription": "지 혼자 넘어지던데 로버트슨이 어쨌다는 거예요?",
                "sound_type": [
                    2
                ],
                "start": 7.924,
                "end": 10.236
            },
            {
                "speaker_id": "1",
                "transcription": "VAR도 내가 미래에 가서 미리 보고 왔어 페널티킥 땅땅땅",
                "sound_type": [
                    2
                ],
                "start": 10.246,
                "end": 13.651
            },
            {
                "speaker_id": "2",
                "transcription": "아니 진짜 뭐 하자는 거예요?",
                "sound_type": [
                    2
                ],
                "start": 13.661,
                "end": 15.097
            },
            {
                "speaker_id": "2",
                "transcription": "@이름1가 로버트슨은 그리지도 않은 것 같은데 뭐 맨유한테 돈이라도 받으신 거예요?",
                "sound_type": [
                    2
                ],
                "start": 15.107,
                "end": 19.397
            },
            {
                "speaker_id": "4",
                "transcription": "어이 페널티킥 차야 되니까 그만하고 좀 나오지?",
                "sound_type": [
                    2
                ],
                "start": 19.407,
                "end": 21.872
            },
            {
                "speaker_id": "2",
                "transcription": "브루노 이 새끼가 너 인마 자꾸 그렇게 다이빙할 거면 수영장 가서 하라고",
                "sound_type": [
                    2
                ],
                "start": 21.882,
                "end": 25.989
            },
            {
                "speaker_id": "3",
                "transcription": "맞아 페널티킥으로 골 세탁하니까 좋냐?",
                "sound_type": [
                    2
                ],
                "start": 25.999,
                "end": 28.100
            },
            {
                "speaker_id": "3",
                "transcription": "이딴식으로 내 득점왕을 위협할 생각이거든 밤길 조심하는 게 좋아.",
                "sound_type": [
                    2
                ],
                "start": 28.110,
                "end": 31.029
            },
            {
                "speaker_id": "5",
                "transcription": "이것들이 우리 브루노한테 뭐 하는 짓이야?",
                "sound_type": [
                    2
                ],
                "start": 31.039,
                "end": 33.450
            },
            {
                "speaker_id": "5",
                "transcription": "그리고 뭐 어떻게 한 건지 나도 잘 모르겠지만 저것도 다 실력이거든?",
                "sound_type": [
                    2
                ],
                "start": 33.460,
                "end": 37.299
            },
            {
                "speaker_id": "5",
                "transcription": "부러우면 너네도 연습하던가",
                "sound_type": [
                    2
                ],
                "start": 37.309,
                "end": 39.247
            },
            {
                "speaker_id": "4",
                "transcription": "저기 주장",
                "sound_type": [
                    2
                ],
                "start": 39.257,
                "end": 40.161
            },
            {
                "speaker_id": "5",
                "transcription": "어 고맙단 말은 됐어 이렇게 대신 싸워주는 게 바로 주장의 역할",
                "sound_type": [
                    2
                ],
                "start": 40.171,
                "end": 44.843
            },
            {
                "speaker_id": "4",
                "transcription": "아뇨 시간도 얼마 안 남았는데 좀 나오시라고요.",
                "sound_type": [
                    2
                ],
                "start": 44.853,
                "end": 47.177
            },
            {
                "speaker_id": "5",
                "transcription": "뭐 뭐라고?",
                "sound_type": [
                    2
                ],
                "start": 47.187,
                "end": 48.152
            },
            {
                "speaker_id": "4",
                "transcription": "무승부로 만족할 거예요?",
                "sound_type": [
                    2
                ],
                "start": 48.162,
                "end": 49.575
            },
            {
                "speaker_id": "4",
                "transcription": "맨유가 우승해야죠.",
                "sound_type": [
                    2
                ],
                "start": 49.585,
                "end": 50.653
            },
            {
                "speaker_id": "4",
                "transcription": "얼른 넣고 한 번 더 넘어져서 페널티킥으로 해트트릭 할 거니까 잠자코 지켜보세요.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 50.663,
                "end": 65.099
            }
        ]
    },
