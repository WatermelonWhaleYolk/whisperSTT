        "term": [
            {
                "speaker_id": "1",
                "transcription": "헤이 헤이 가이즈 스페인 이즈 좆밥 오케이?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 0.000,
                "end": 2.586
            },
            {
                "speaker_id": "2",
                "transcription": "얘들아 스페인이 강팀이라지만 우리가 이길 수 있어.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 2.596,
                "end": 5.458
            },
            {
                "speaker_id": "1",
                "transcription": "지성 기현 정환 맞고 뒤져라 슛 오케이?",
                "sound_type": [
                    2,
                    3
                ],
                "start": 5.468,
                "end": 9.313
            },
            {
                "speaker_id": "2",
                "transcription": "지성이하고 기현이 정환이는 더 공격적으로 나서고",
                "sound_type": [
