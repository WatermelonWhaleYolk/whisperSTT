        "term": [
            {
                "speaker_id": "1",
                "transcription": "그때 2번 수술한 내 오른쪽 팔꿈치가 완전히 죽었다는 걸 알았다.",
                "sound_type": [
                    2,
                    3,
                    5
                ],
                "start": 0.000,
                "end": 7.568
            },
            {
                "speaker_id": "1",
                "transcription": "그 끊어지는 소리는 타격음보다 둔탁했다.",
                "sound_type": [
                    2,
                    3
                ],
                "start": 7.578,
                "end": 14.717
            },
            {
                "speaker_id": "2",
                "transcription": "한태황 승은 날아가고 시합은 연장으로 들어갑니다.",
                "sound_type": [
                    2,
                    3,
                    4
                ],
                "start": 14.727,
                "end": 20.532
            },
            {
                "speaker_id": "3",
                "transcription": "에이 씨 또 날아갔네.",
                "sound_type": [
                    2,
                    4
                ],
                "start": 20.542,
                "end": 23.453
            },
            {
                "speaker_id": "3",
                "transcription": "이게 몇 번째야?",
                "sound_type": [
                    2,
                    4
                ],
                "start": 23.463,
                "end": 25.134
            },
            {
                "speaker_id": "3",
                "transcription": "존나게 던져봤자 존나게 삽질이지.",
